                {
                    "type": "archive",
                    "url": "http://cairographics.org/releases/pycairo-1.10.0.tar.bz2",
                    "sha256": "9aa4078e7eb5be583aeabbe8d87172797717f95e8c4338f0d4a17b683a7253be"
